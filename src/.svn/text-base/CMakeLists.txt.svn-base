GET_TARGET_PROPERTY(T61DST_LIBRARY T61DST LOCATION)
GET_TARGET_PROPERTY(T61ANA_LIBRARY T61ANA LOCATION)
GET_TARGET_PROPERTY(RootDS_LIBRARY RootDS LOCATION)

set(root61_SOURCES T49Main.C)

LIST(APPEND root61_INCLUDE_DIRECTORIES ${CMAKE_SOURCE_DIR}/T61DST/inc)
INCLUDE_DIRECTORIES(${root61_INCLUDE_DIRECTORIES})

add_executable(root61 ${root61_SOURCES})

LIST(APPEND root61_LINKED_LIBRARIES T61DST T61ANA ${ROOT_LIBS})
if (NOT DISABLE_DSPACK)
  LIST(APPEND root61_LINKED_LIBRARIES RootDS)
endif (NOT DISABLE_DSPACK)
TARGET_LINK_LIBRARIES(root61 ${root61_LINKED_LIBRARIES})

INSTALL(TARGETS root61 DESTINATION bin)
